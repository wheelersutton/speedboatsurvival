void goToPause() {
 state = PAUSE;
 *(unsigned short *)0x4000000 = 3 | (1<<10);
 fillScreen(0);
 drawBackgroundImage3(pausescreenBitmap);
 playSoundB(pausesound, 6544, 11025, 0);
 pauseSoundA();
}
void pause() {
 if ((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
  state = GAME;
  initializeGame();
  unpauseSoundA();
 }
 if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1))))) {
  state = START;
  goToStart();
 }
}


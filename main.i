 for (int i = 0; i < 5; i++) {
  itoa(highScores[i], buffer, 10);
  drawString(50 + i*15, 100, buffer, ((31) | (31)<<5 | (31)<<10));
  sprintf(buffer, initials[i]);
  drawString(50 + i*15, 140, buffer, ((31) | (31)<<5 | (31)<<10));
  if (i == myScore) {
   sprintf(buffer, "*");
   drawString(50 + i*15, 63, buffer, ((0) | (31)<<5 | (0)<<10));
   drawString(50 + i*15, 160, buffer, ((0) | (31)<<5 | (0)<<10));
  }
 }
}

void updateHighScores() {
 for (int i = 0; i < 5; i++) {
  if (highScores[i] < boat.kills) {
   for (int j = 4; j > i; j--) {
    highScores[j] = highScores[j-1];
    initials[j] = initials[j-1];
   }
   highScores[i] = boat.kills;


   finalInitials[0] = letters[0];
   finalInitials[1] = letters[1];
   finalInitials[2] = letters[2];

   initials[i] = finalInitials;
   myScore = i;
   break;
  }
 }
}

void scores() {
 if ((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
  playSoundB(menubutton, 8821, 11025, 0);
  goToStart();
 }
}

void update() {
 updateBoat(&boat);







 difficultyReducer = (boat.kills % 10);
 if (difficultyReducer == 0 && prevBoatKills != boat.kills) {
  difficulty -= 50;
  if (difficulty <= 50) {
   difficulty = 50;
  }
  prevBoatKills = boat.kills;
 }

 int random = rand() % difficulty;
 if (frame % random == 0) {
  fireObstacle(&obstacles[rand()%(40 -1)], &boat);
 }

 for (int i = 0; i < 40; i++) {
  updateObstacle(&obstacles[i], frame);
  playerObstacleCollision(&boat, &obstacles[i]);
 }

 for (int i = 0; i < 20; i++) {
  updateBullet(&bullets[i]);
 }

 for (int i = 0; i < 20; i++) {
  for (int j = 0; j < 40; j++) {
   obstacleBulletCollision(&bullets[i], &obstacles[j], &boat);
  }
 }


 if (boat.active == 0) {
  goToLose();
 }


 if (frame % 2 == 0) {
  BG0hOff++;
 }
 if (frame % 4 == 0) {
  BG1hOff++;
 }


 if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
  int pCol = ((boat.col) >> 8);



  if (prevBullet == -1 || pCol + 20 <= bullets[prevBullet].col || !bullets[prevBullet].active) {
   playSoundB(bullet, 4525, 11025, 0);
   fireBullet(&bullets[bulletCount], &boat);
   prevBullet = bulletCount;
   bulletCount++;

   if (bulletCount >= 20 - 1) {
    bulletCount = 0;
   }
  }
 }


 if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && !boat.shieldEnable) {
  boat.shieldEnable = 1;
  boat.spriteCol = 9;
 } else if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && boat.shieldEnable) {
  boat.shieldEnable = 0;
  boat.spriteCol = 1;
 }


 if ((~(*(volatile unsigned int *)0x04000130) & ((1<<4)))) {
  if (frame % 10 == 0) {
   boat.spriteRow++;
   if (boat.spriteRow == 7) {
    boat.spriteRow = 0;
   }
  }
 } else {
  boat.spriteRow = 0;
 }

}
